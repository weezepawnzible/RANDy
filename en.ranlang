<lang name="English" filename="en.ranlang" version="1.0.0">
				<!-- start -->			
	<section name="start">
		<start_header_row_1>Generate a random number</start_header_row_1>
		<start_header_row_2>between 1 and ...</start_header_row_2>
		<start_input_choice_1>To choose a number press its assigned key, which is showed in the square brackets []</start_input_choice_1>
	</section>
				<!-- prep_choice_name_values -->
	<section name="prep_choice_name_values">
		<prep_choice_name_values_header_row_1>Do you want to enter a title for each numeric value?</prep_choice_name_values_header_row_1>
		<prep_choice_name_values_choice_option_1>Yes</prep_choice_name_values_choice_option_1>
		<prep_choice_name_values_choice_option_2>No</prep_choice_name_values_choice_option_2>
