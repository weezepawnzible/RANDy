	</section>	
				<!-- choice_name_values -->		
	<section name="choice_name_values">
		<choice_name_values_header_row_1>Name values</choice_name_values_header_row_1>
		<choice_name_values_input_name_1>Please enter a name for number</choice_name_values_input_name_1>
	</section>	
